    "import re\n",
    "\n",
    "def remove_text1(veri):\n",
    "    sayilar = [float(s) for s in re.findall(r'\\d+', str(veri))]\n",
    "    \n",
    "    if len(sayilar) == 2:\n",
    "        return sum(sayilar) / 2\n",
    "    \n",
    "    elif len(sayilar) == 1:\n",
    "        return sayilar[0]\n",
    "    \n",
    "    return None\n",
    "\n",
    "df_encoded['Binanın_Yaşı'] = df_encoded['Binanın_Yaşı'].apply(remove_text1).astype(float)"
   ]
  },
